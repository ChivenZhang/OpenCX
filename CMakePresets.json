{
  "version": 6,
  "configurePresets": [
    {
      "name": "x64-Debug",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/Output/${presetName}",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",

        "OPENCX_ENABLE_SAMPLE": "ON"
      }
    },
    {
      "name": "x64-Release",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/Output/${presetName}",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "RelWithDebInfo",

        "OPENCX_ENABLE_SAMPLE": "ON"
      }
    }
  ]
}